<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns="http://cbml.googlecode.com/svn/trunk/jc3iedmOnt/rootClassOnt/jc3iedm_PLAN-ORDERExtended.owl#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xml:base="http://cbml.googlecode.com/svn/trunk/jc3iedmOnt/rootClassOnt/jc3iedm_PLAN-ORDERExtended.owl#">
  <owl:Ontology rdf:about="#">
    <owl:imports rdf:resource="jc3iedm_PLAN-ORDER.owl#"/>
  </owl:Ontology>
  <owl:Class rdf:about="jc3iedm_PLAN-ORDER.owl#PLAN-ORDER">
    <rdfs:comment xml:lang="en">A planned or ordered scheme worked out beforehand for the accomplishment of an operational objective.</rdfs:comment>
    <rdfs:label xml:lang="en">plan order</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="#ORDER_Administrative/Logistics_order">
    <rdfs:subClassOf>
      <owl:Class rdf:about="jc3iedm_PLAN-ORDER.owl#ORDER"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:hasValue xml:lang="en">ADLOGO</owl:hasValue>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:about="jc3iedm_PLAN-ORDER.owl#order-category-code"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:label xml:lang="en">administrative/logistics order</rdfs:label>
    <rdfs:comment xml:lang="en">An order to give the commanderâ€™s plan for administrative and service support of operations.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="ORDER_Operations_order">
    <rdfs:subClassOf>
      <owl:Class rdf:about="jc3iedm_PLAN-ORDER.owl#ORDER"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:hasValue xml:lang="en">OPORD</owl:hasValue>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:about="jc3iedm_PLAN-ORDER.owl#order-category-code"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:label xml:lang="en">operations order</rdfs:label>
    <rdfs:comment xml:lang="en">An order that includes only such detail as is necessary for commanders of subordinate formations/units to issue their own orders and to ensure coordination.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="PLAN_Operations_plan">
    <rdfs:subClassOf>
      <owl:Class rdf:about="jc3iedm_PLAN-ORDER.owl#PLAN"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:hasValue xml:lang="en">OPLAN</owl:hasValue>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:about="jc3iedm_PLAN-ORDER.owl#plan-category-code"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:label xml:lang="en">operations plan</rdfs:label>
    <rdfs:comment xml:lang="en">A mechanism which a command uses to plan/prepare to conduct military operations.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="ORDER_Fragmentary_order_to_existing_operation_order">
    <rdfs:subClassOf>
      <owl:Class rdf:about="jc3iedm_PLAN-ORDER.owl#ORDER"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:hasValue xml:lang="en">FRAGO</owl:hasValue>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:about="jc3iedm_PLAN-ORDER.owl#order-category-code"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:label xml:lang="en">fragmentary order to existing operation order</rdfs:label>
    <rdfs:comment xml:lang="en">An abbreviated form of an Operations order.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="jc3iedm_PLAN-ORDER.owl#PLAN">
    <rdfs:comment xml:lang="en">A proposal for putting into effect a command decision or project; it represents the preparation for future or anticipated operations.</rdfs:comment>
    <rdfs:label xml:lang="en">plan</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:about="jc3iedm_PLAN-ORDER.owl#plan-order-category-code"/>
        </owl:onProperty>
        <owl:hasValue xml:lang="en">PLAN</owl:hasValue>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="jc3iedm_PLAN-ORDER.owl#PLAN-ORDER"/>
  </owl:Class>
  <owl:Class rdf:about="jc3iedm_PLAN-ORDER.owl#ORDER">
    <rdfs:comment xml:lang="en">A communication that conveys instructions from a superior to a subordinate.</rdfs:comment>
    <rdfs:label xml:lang="en">order</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:about="jc3iedm_PLAN-ORDER.owl#plan-order-category-code"/>
        </owl:onProperty>
        <owl:hasValue xml:lang="en">ORDER</owl:hasValue>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="jc3iedm_PLAN-ORDER.owl#PLAN-ORDER"/>
  </owl:Class>
  <owl:Class rdf:ID="ORDER_Warning_order">
    <rdfs:subClassOf rdf:resource="jc3iedm_PLAN-ORDER.owl#ORDER"/>
