      <owl:Restriction>
        <owl:hasValue xml:lang="en">WNGO</owl:hasValue>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:about="jc3iedm_PLAN-ORDER.owl#order-category-code"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:label xml:lang="en">warning order</rdfs:label>
    <rdfs:comment xml:lang="en">An order to assist units and their staffs initiate the preparations for and the execution of their new mission by giving them the maximum warning and essential details of impending operations and information of time available.</rdfs:comment>
  </owl:Class>
  <owl:DatatypeProperty rdf:about="jc3iedm_PLAN-ORDER.owl#plan-category-code">
    <rdfs:domain rdf:resource="jc3iedm_PLAN-ORDER.owl#PLAN"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="jc3iedm_PLAN-ORDER.owl#order-category-code">
    <rdfs:domain rdf:resource="jc3iedm_PLAN-ORDER.owl#ORDER"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="jc3iedm_PLAN-ORDER.owl#plan-order-category-code">
    <rdfs:domain rdf:resource="jc3iedm_PLAN-ORDER.owl#PLAN-ORDER"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
</rdf:RDF>
