    <!-- http://cbml.googlecode.com/svn/trunk/basicCommOnt.owl#Acknowledgement -->

    <owl:Class rdf:about="&basicCommOnt;Acknowledgement">
        <rdfs:subClassOf rdf:resource="&upperC2Ont;CommunicationAct"/>
        <rdfs:isDefinedBy>&#39;The action or an act of acknowledging, confessing, admitting, or owning something; admission, confession; an instance of this.&#39;
