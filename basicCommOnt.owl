    </owl:Class>
    


    <!-- http://cbml.googlecode.com/svn/trunk/basicCommOnt.owl#Declarative -->

    <owl:Class rdf:about="&basicCommOnt;Declarative">
        <rdfs:subClassOf rdf:resource="&upperC2Ont;CommunicationAct"/>
        <rdfs:isDefinedBy>&#39;Communication acts which brings about the correspondence of the
