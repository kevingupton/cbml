    <!-- http://cbml.googlecode.com/svn/trunk/basicCommOnt.owl#Assertive -->

    <owl:Class rdf:about="&basicCommOnt;Assertive">
        <rdfs:subClassOf rdf:resource="&upperC2Ont;CommunicationAct"/>
        <rdfs:isDefinedBy>&#39;Communication acts that express the sender’s attitude of committing to
the receiver to the veracity of the propositional content&#39;
&lt;Jesús Bermúdez, Alfredo Goñi, Arantza Illarramendi, and Simone Santini. 2007. Is an OWL ontology adequate for foreign software agents communication?. Applied Ontology 2, 3-4 (August 2007)&gt;</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- http://cbml.googlecode.com/svn/trunk/basicCommOnt.owl#Commissive -->

    <owl:Class rdf:about="&basicCommOnt;Commissive">
        <rdfs:subClassOf rdf:resource="&upperC2Ont;CommunicationAct"/>
        <rdfs:isDefinedBy>&#39;Communication acts that expresses the sender’s attitude of obligating
himself to do something, perhaps under certain conditions&#39;
&lt;Jesús Bermúdez, Alfredo Goñi, Arantza Illarramendi, and Simone Santini. 2007. Is an OWL ontology adequate for foreign software agents communication?. Applied Ontology 2, 3-4 (August 2007)&gt;</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- http://cbml.googlecode.com/svn/trunk/basicCommOnt.owl#ContentDevelopment -->

