    </owl:Class>
    


    <!-- http://cbml.googlecode.com/svn/trunk/basicCommOnt.owl#Agent -->

    <owl:Class rdf:about="&basicCommOnt;Agent">
        <rdfs:subClassOf rdf:resource="&upperC2Ont;Entity"/>
        <rdfs:isDefinedBy>&#39;One who (or that which) acts or exerts power, as distinguished from the patient, and also from the instrument.&#39;
&lt;http://www.oed.com/viewdictionaryentry/Entry/3859&gt;</rdfs:isDefinedBy>
